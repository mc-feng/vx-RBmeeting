.cover{
  background:rgba(0,0,0,0.4);
  position:fixed;
  top:0;
  left:0;
  right:0;
  bottom:0;
  
  /* justify-content:center;

align-items:center;

display:flex; */
}
.box{
   margin: auto;
     position: fixed;
     left: 0;
     right: 0;
  /* position:absolute;
  top:50%;
  left:50%;
  transform:translate(-50%,-50%); */
  /* position:absolute;
  left:50%;
  transform:translate(-50%,0); */
  background:#fff;
  border-radius: 18rpx;
}
.box-container{
  position:relative;
}
.tips{
  width:160rpx;
  height:160rpx;
  border-radius:50%;
  position:absolute;
  top:-80rpx;
  left:50%;
  transform:translate(-50%,0);
  background:#F03F24;
}
.tips-box{
  position:relative;
  width:100%;
  height:100%;
}
.msg-icon:before{
  content:'\e613';
  font-weight:600;
  position:absolute;
  top:50%;
  left:50%;
  transform:translate(-50%,-50%);
  font-size:90rpx;
  color:#fff;
}
.right-icon:before{
  content:'\e611';
  font-weight:600;
  position:absolute;
  top:50%;
  left:50%;
  transform:translate(-50%,-50%);
  font-size:90rpx;
  color:#fff;
}
.wrong-icon:before{
  content:'\e64a';
  font-weight:600;
  position:absolute;
  top:50%;
  left:50%;
  transform:translate(-50%,-50%);
  font-size:90rpx;
  color:#fff;
}
.modals-cancel{
  width:100%;
  height:100%;
}